{
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
        "clsx",
        "classnames"
    ],
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "css",
    "proseWrap": "always"
}